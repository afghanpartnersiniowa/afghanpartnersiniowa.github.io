{%- set absolutePageUrl -%}{{ page.url | url | absoluteUrl(env.url) }}{%- endset -%}
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="preload" href="/css/page.css" as="style">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ pageTitle or title }} - {{ site.title }}</title>
<meta name="description" content="{{ description or site.description }}">
<meta name="author" content="{{ site.author }}">
<meta name="robots" content="index,follow">
<!-- Social Metadata -->
<meta property="og:site_name" content="{{ site.author }}">
<meta property="og:title" content="{{ title or site.title }} - {{ site.title }}">
<meta property="og:description" content="{{ description or site.description }}">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ absolutePageUrl }}">
<meta property="og:image" itemprop="image primaryImageOfPage" content="{{ site.image | url | absoluteUrl(env.url) }}">
<meta property="og:image:type" content="image/png">
<meta property="og:image:width" content="512" />
<meta property="og:image:height" content="512" />
<meta property="og:image:alt" content="The Afghan Partners in Iowa logo. The American and Afghanistan flags.">
<meta name="twitter:card" content="summary">
<meta name="twitter:domain" content="www.afghanpartnersiniowa.org">
<meta name="twitter:title" property="og:title" itemprop="name" content="{{ title or site.title }} - {{ site.title }}">
<meta name="twitter:description" property="og:description" itemprop="description" content="{{ description or site.description }}">
<meta name="theme-color" content="#ffffff">
<link rel="shortcut icon" href="{{ site.favicon }}">
<link rel="apple-touch-icon" href="{{ site.image }}">
<link rel="image_src" href="{{ site.image }}">
<link rel="stylesheet" href="/css/page.css">
<script defer src="/js/alpine.js"></script>