    "cssnano": "^5.1.15",
    "del-cli": "^5.1.0",
    "eleventy-favicon": "^1.1.3",
    "html-minifier": "^4.0.0",
    "markdown-it": "^13.0.2",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-emoji": "^2.0.2",
